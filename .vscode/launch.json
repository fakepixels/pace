{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Pace CLI",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "console": "integratedTerminal"
    },
    {
      "name": "Launch Pace CLI Server",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": ["--serve"],
      "console": "integratedTerminal"
    }
  ]
}